{
  "canisters": {
    "cosmos_rpc": {
      "type": "custom",
      "candid": "src/ic-cosmos-rpc/ic-cosmos-rpc.did",
      "wasm": "ic-cosmos-rpc.wasm.gz",
      "init_arg": "(record {})",
      "gzip": true
    },
    "cosmos_rpc_demo": {
      "type": "custom",
      "candid": "src/ic-cosmos-rpc/ic-cosmos-rpc.did",
      "wasm": "ic-cosmos-rpc.wasm.gz",
      "gzip": true,
      "init_arg": "(record {demo = opt true})"
    },
    "cosmos_wallet": {
      "type": "custom",
      "candid": "src/ic-cosmos-wallet/ic-cosmos-wallet.did",
      "wasm": "ic-cosmos-wallet.wasm.gz",
      "gzip": true,
      "optimize": "cycles"
    },
    "e2e": {
      "dependencies": ["cosmos_rpc", "cosmos_wallet"],
      "type": "rust",
      "package": "e2e",
      "candid": "src/e2e/e2e.did",
      "gzip": true
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.24.1",
  "version": 1
}
